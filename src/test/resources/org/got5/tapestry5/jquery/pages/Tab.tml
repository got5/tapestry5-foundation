  <span class="tag">&lt;<span class="title">li</span> <span class="attribute">class</span>=<span class="value">&quot;tab-title&quot;</span> <span class="attribute">role</span>=<span class="value">&quot;presentational&quot;</span>&gt;</span><span class="tag">&lt;<span class="title">a</span> <span class="attribute">href</span>=<span class="value">&quot;#panel2-3&quot;</span> <span class="attribute">role</span>=<span class="value">&quot;tab&quot;</span> <span class="attribute">tabindex</span>=<span class="value">&quot;0&quot;</span> <span class="attribute">aria-selected</span>=<span class="value">&quot;false&quot;</span> <span class="attribute">controls</span>=<span class="value">&quot;panel2-3&quot;</span>&gt;</span>Tab 3<span class="tag">&lt;/<span class="title">a</span>&gt;</span><span class="tag">&lt;/<span class="title">li</span>&gt;</span>
